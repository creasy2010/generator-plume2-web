{
  "compilerOptions": {
    "sourceMap": false,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "rootDirs": [
      "src"
    ],
    "outDir": "./build/",
    "lib": [
      "es2015",
      "es2015.promise",
      "es7",
      "es2017",
      "dom"
    ],
    "paths": {
      "qmkit": [
        "./web_modules/qmkit"
      ],
      "qmkit/*": [
        "web_modules/qmkit/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}